// RUN: %file-check -vv -check-prefix=SW2 %s --input-file %t.c
// SW2: switch (
// SW2: case {{[0-9]+[U]?}}:
// SW2: case {{[0-9]+[U]?}}:
// SW2: __builtin_unreachable
// SW2-NOT: default:
// SW2-NOT: goto *
{
  "architecture": "ARM",
  "id": "ARM:LE:32:Cortex",
  "format": "Executable and Linking Format (ELF)",
  "functions": {
    "ram:16000000": {
